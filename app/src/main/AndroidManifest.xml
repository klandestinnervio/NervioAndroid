<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nerv_io">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NervIO">
        <activity android:name=".view.GuideBookActivity" />
        <activity android:name=".view.ResultDiagnosticActivity" />
        <activity android:name=".view.SignInActivity" android:theme="@style/Theme.NervIO.NothingToolbar"/>
        <activity android:name=".view.HospitalRecomActivity" />
        <activity android:name=".view.DiagnosticTestActivity" />
        <activity android:name=".view.HistoryDiagnosisActivity" />
        <activity android:name=".view.MenuActivity" android:theme="@style/Theme.NervIO.NothingToolbar"/>
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>